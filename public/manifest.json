{
	"manifest_version": 3,
	"name": "Mockinator",
	"version": "3.0.0",
	"description": "Перехват fetch-запросов и возврат mock-данных.",
	"permissions": ["storage", "scripting", "activeTab"],
	"host_permissions": ["<all_urls>"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html"
	}
}
