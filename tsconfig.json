{
	"compilerOptions": {
		"target": "ES6",
		"module": "NodeNext",
		"jsx": "react",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "NodeNext",
		"types": ["vite/client"],
		"lib": ["es2017", "dom"]
	},
	"exclude": ["node_modules", "dist"]
}
